{
    "name": "fsi/metadata",
    "type": "library",
    "description": "FSi Metadata Component is a library that provide some commonly used mechanisms to read configurations for classes from different sources like annotations, yaml or xml.",
    "keywords": ["fsi", "component", "metadata", "annotation"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@fsi.pl"
        },
        {
            "name": "Lukasz Cybula",
            "email": "lukasz@fsi.pl"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "doctrine/common": "~2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "FSi\\Component\\Metadata": "lib/",
            "FSi\\Component\\Metadata\\Tests": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
